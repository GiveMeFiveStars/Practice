<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.mapper.MachineMapper">

    <resultMap id="BaseResultMap" type="com.system.pojo.Machine">
            <id property="mId" column="m_id" jdbcType="INTEGER"/>
            <result property="mName" column="m_name" jdbcType="VARCHAR"/>
            <result property="modelNum" column="model_num" jdbcType="VARCHAR"/>
            <result property="mStatus" column="m_status" jdbcType="INTEGER"/>
            <result property="stashId" column="stash_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        m_id,m_name,model_num,
        m_status,stash_id
    </sql>
</mapper>
